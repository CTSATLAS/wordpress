{% extends base_twig %} 
{% block features %}{% endblock %} 
{% block mission %}{% endblock %} 
{% block articles %}{% endblock %} 
{% block content %}

    <div class="content-wrapper">
        <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
            <section class="article-content">
                <h1 class="article-h1">{{post.title}} ....</h1>

                <div class="article-body">
                    {{post.content}}
                </div>
            </section>
                {{ events_url }}
                <pre>
                   {{ dump(events) }} 
                </pre>
            
            <ul>

                {% for event in events %}
                    <li><a href="{{ event.permalink }}">{{ event.title }}</a></li>
                    {% else %}
                    <li>Sorry, no posts matched your criteria</li>
                    {% endfor %}
            </ul>                   
            <section>

            </section>

        </article>
    </div>
    <!-- /content-wrapper -->
{% endblock %}